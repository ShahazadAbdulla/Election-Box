#include <Wire.h>#include <LiquidCrystal_I2C.h>#define LCD_ADDRESS 0x27  // Change this to your detected addressLiquidCrystal_I2C lcd(LCD_ADDRESS, 16, 2);  // 16x2 LCDvoid setup() {  Serial.begin(9600);  Serial.println("Initializing LCD...");  Wire.begin();  lcd.init();    lcd.backlight();  Serial.println("LCD initialized successfully.");  Serial.println("Attempting to print...");  lcd.setCursor(0, 0);  lcd.print("Hello, World!");  Serial.println("Message printed. Check LCD.");}void loop() {}